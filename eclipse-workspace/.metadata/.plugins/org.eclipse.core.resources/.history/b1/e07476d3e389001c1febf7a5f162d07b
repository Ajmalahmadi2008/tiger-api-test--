package second.week.ofjava;

public class StringToUpperCase {
	
	public static void main(String[] args) {
		
		
		// To Upper Case will change case to all UPPERCASE
		
		String city = "Woodbridg";
		
		String cityInAllCaps = city.toUpperCase();
		
		System.out.println(cityInAllCaps);
		
		String country = "united state of America";
		
		String countyInAllcaps = country.toUpperCase();
		
		System.out.println(countyInAllcaps);
		
	}

	
	// To Upper Case will Change Case to all UPPERCASE.
	
	
}
